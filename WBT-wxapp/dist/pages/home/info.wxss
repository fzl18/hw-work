.bg {
  height: 100%;
  background: #EDEDED;
  padding: 4%;
}
.box {
  background: #fff;
  border-radius: 8px;
  margin-bottom: 4%;
  padding: 4%;
}
.box .tit {
  font-size: 16px;
}
.box .ctx {
  color: #777;
  font-size: 12px;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
view.title {
  background-color: #f5f5f5;
  font-size: 14px;
  font-weight: bold;
  line-height: 50px;
  padding-left: 20px;
  border-left: 3px solid #1986E1;
}
.unlock,
.lock {
  padding: 3px 8px;
  background: #00A0EA;
  border-radius: 8px;
  color: #fff;
  font-size: 12px;
  text-align: center;
  max-width: 80rpx;
}
.lock {
  background: #09BA07;
}
.text {
  line-height: 50px;
}

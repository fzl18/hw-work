<view>
  <view class="content_wrap">
    <view class="head">
      <ui-blur height="200" url="{{ url }}" _pid="{{_pid}}">
        <view class="top_content">
          <view>
            <image src="{{ url }}" mode="aspectFill" style="width:60px;height:60px" />
          </view>
          <view class="name">
            {{name}}
          </view>
        </view>
      </ui-blur>
    </view>
    <view class="content_list">
      <view style="margin-top:5%">
        <navigator url="/pages/home/setCompany" class="ui-row" style="">
          <view class="ui-col" style="">
            <view class="ui-col-content">
              <view>
                <button type="primary">修改企业名片 </button>
              </view>
            </view>
          </view>
        </navigator>
      </view>
      <view style="margin-top:8%">
        <navigator url="/pages/home/setPassword" class="ui-row" style="">
          <view class="ui-col" style="">
            <view class="ui-col-content">
              <view>
                <button type="warn">修改密码 </button>
              </view>
            </view>
          </view>
        </navigator>
      </view>
    </view>
  </view>
</view>